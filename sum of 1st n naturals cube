def cube(n):
    return n*(n**2)

def cubenatnum(n):
    total=0
    if n==0:
        return 0
    elif n==1:
        return 1
    else:
        while(n>1):
            total = total+cube(n)
            n=n-1
        print(total)

num1= int(input("Enter the value to find its sum of n first cube natural numbers"))
cubenatnum(num1)
